{
  "name": "vshn-gatsby",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "npm run lint:js & npm run lint:styles & npm run lint:md",
    "lint:fix": "npm run lint:js:fix & npm run lint:styles:fix & npm run lint:md:fix",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:js:fix": "eslint . --cache --fix --ignore-path .gitignore",
    "lint:styles": "stylelint '{**/*,*}.{css,sass,scss,md,html}' --ignore-path .gitignore",
    "lint:styles:fix": "stylelint '{**/*,*}.{css,sass,scss,md,html}' --cache --fix --ignore-path .gitignore",
    "lint:md": "markdownlint --ignore-path .gitignore .",
    "lint:md:fix": "markdownlint --fix --ignore-path .gitignore ."
  },
  "dependencies": {
    "@svgr/webpack": "^5.4.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.24.53",
    "gatsby-image": "^2.4.16",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-plugin-manifest": "^2.4.27",
    "gatsby-plugin-offline": "^3.2.26",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-plugin-svgr-svgo": "^1.0.12",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-sharp": "^2.5.14",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "markdownlint": "^0.20.4",
    "stylelint": "^13.7.0",
    "stylelint-config-recess-order": "^2.1.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0"
